{
  "name": "302-ai-studio",
  "version": "0.1.0",
  "private": true,
  "description": "Electron + SvelteKit Monorepo Application",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:svelte": "node scripts/build-svelte.js",
    "build:electron": "pnpm build:svelte && pnpm --filter @302-ai-studio/electron-app build",
    "build:electron:mac": "pnpm build:svelte && pnpm --filter @302-ai-studio/electron-app build:mac",
    "build:electron:win": "pnpm build:svelte && pnpm --filter @302-ai-studio/electron-app build:win",
    "build:electron:linux": "pnpm build:svelte && pnpm --filter @302-ai-studio/electron-app build:linux",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "postinstall": "pnpm rebuild electron",
    "setup": "pnpm install && pnpm rebuild electron",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "workspaces": [
    "app/*",
    "packages/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.3.3",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.15.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "electron"
    ]
  }
}
